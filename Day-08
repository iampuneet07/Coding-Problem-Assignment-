//Day-08

// 1st question
#include<bits/stdc++.h>
using namespace std;
int stringLength(string &str)
{
    return str.length();
}
int main()
{
    string str;
    getline(cin,str);
    cout<<stringLength(str);
    return 0;
}

//2nd question
#include<bits/stdc++.h>
using namespace std;
int countCharacter(string &str,char ch)
{
    int count=0;
    for(int i=0;i<str.length();i++)
    {
        if(ch==str[i])
        {
            count++;
        }
    }
    return count;
}
int main()
{
    string str;
    getline(cin,str);
    char ch;
    cin>>ch;
    cout<<countCharacter(str,ch);
    return 0;
}

//3rd question
#include<bits/stdc++.h>
using namespace std;
string concatenateStrings(string &str1,string &str2)
{
    string str3=str1+str2;
    return str3;
}
int main()
{
    string str1;
    getline(cin,str1);
    string str2;
    getline(cin,str2);
    cout<<concatenateStrings(str1,str2);
    return 0;
}
